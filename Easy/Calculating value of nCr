Input     5     n  
          2     r
          
Output   10

#include <iostream>
using namespace std;

int fact(int num)
{
    int factorial =1;
    for(int i=2;i<=num;i++)
    {
        factorial *=i;
    }
    return factorial;
}

int main() {
    int n;
    cin>>n;

    int ans = fact(n) / ( fact(r) * fact(n-r) );
    cout<<ans;
}
