Input     4             5         <- n

Output   24             120

#include <iostream>
using namespace std;

int fact(int num)
{
    int factorial =1;
    for(int i=2;i<=num;i++)
    {
        factorial *=i;
    }
    return factorial;
}

int main() {
    int n;
    cin>>n;

    int ans=fact(n);
    cout<<ans;
}
